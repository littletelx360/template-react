{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "repository": {
    "url": "<%= username %>/<%= name %>",
    "type": "git"
  },
  "scripts": {
    "dev": "poi",<% if (xo || test) { %>
    "test": "xo<% if (test) { %> && react-jest --env=jsdom<% } %>",<% } %>
    "build": "poi build"<% if (xo) { %>,
    "lint": "xo",
    "lint:fix": "xo --fix"<% } %>
  },
  "author": "<%= username %> <<%= email %>>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-react-require": "^3.0.0",
    "babel-preset-react-app": "^2.1.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",<% if (test) { %>
    "react-jest": "^0.2.0",<% } %><% if (enzyme) { %>
    "react-test-renderer": "^15.5.4",
    "enzyme": "^2.8.2",<% } %>
    "react-hot-loader": "next"<% if (poi || offline) { %>,
    "poi": "^8.0.0-rc.4"<% } %><% if (offline) { %>,
    "offline-plugin": "^4.6.1"<% } %><% if (xo) {%>,
    "babel-eslint": "^7.2.2",
    "eslint-config-vue-app": "^1.2.1",
    "xo": "^0.18.1"<% } %>
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }<% if (xo) { %>,
  "xo": {
    "parser": "babel-eslint",
    "extends": "vue-app",
    "rules": {
      "unicorn/filename-case": 0
    }
  }<% } %>
}
